  padding: 25rpx 0rpx;
  border-bottom: 2rpx solid #eee;
}
.yh-mast-row:last-of-type{
  border-bottom: 0rpx solid #eee;
}
.yh-mast-row .icons{
  flex: 1
}
.yh-mast-row .icons image{
  width: 72rpx;
  height: 72rpx;
  border-radius: 50%;
}
.yh-mast-row .yh-mast-col{
  flex: 11;
