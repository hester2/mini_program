padding: 25rpx 0px;
margin-bottom: 19rpx;
margin-left: 25rpx !important;
color: #4D4D4D;

}
.yh-mast-grids .yh-mast-cols:nth-of-type(1) {
margin-left: 0px !important;

}

.yh-mast-grids .yh-mast-cols .yh-mast-items{
