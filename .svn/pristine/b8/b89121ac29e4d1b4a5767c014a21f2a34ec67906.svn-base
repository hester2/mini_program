  font-weight: bold
}
.yh-mast-titles{
  font-size: 22rpx;
  text-align: center;
}
.yh-mast-lo{
  display: flex;
  width: 100%;
}
.size{
  color: #ccc
}
.yh-mast-le{
  display: flex;
  align-items: center;
  color: #4D4D4D;
  margin-top: 10rpx; 
}
.yh-mast-le text:nth-of-type(1){
  font-size: 28rpx;
  display: block;
}
.yh-mast-le text:nth-of-type(2){
  font-size: 22rpx;
