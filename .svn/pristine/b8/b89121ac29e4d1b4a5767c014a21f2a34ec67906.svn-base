  flex-direction: column;
  margin-left: 20rpx;
}
.yh-mast-lc{
  display: flex;
  align-items: center;
  color: #4D4D4D
}
.yh-mast-lc text{
  display: block;
margin-top: 0px;
margin-right: 20rpx;

}
.yh-mast-lc text:nth-of-type(1){
