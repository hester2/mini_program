
/* 提示 */

.yh-cation-main{
  margin: 28rpx 28rpx;
  box-shadow: 3rpx 3rpx 16rpx #f1f1f1;
  box-sizing: border-box;
  padding: 28rpx 28rpx;
}

.word-class{

  font-size: 14px;

  color: rgb(95, 87, 87);

  margin-left: 10px;

}

/* 选择图片 */

.img_box{

  width:690rpx;

  position:relative;

  display: flex;

  flex-wrap: wrap;

  margin:0 auto;

  padding-top: 20px;

}

.imgs{

  width:25%;

  display: flex;

  justify-content: center;

  margin-bottom:20rpx;

}

.imgs image{

  width:135rpx;

  height: 135rpx;

  border:1px solid rgba(214, 212, 212, 0.1);

}

.imgs .images{

  position:relative;

}

.images button{

  width:100%;

  height:100%;

  position:absolute;

  top:0;

  left:0;

}

.img_box .images{

  width:135rpx;

height: 135rpx;


  border-radius:4rpx;

  display: flex;

  align-items: center;

  justify-content: center;

}

.img_box .images>image{

  width: 100%;
  height: 100%

}

/* 上传按钮 */

.UploadBtnarea{

  width: 100%;

  height: 32px;

  margin-top: 30px;

  margin-bottom: 10px;

}

.UploadBtnclass{

  height: 100%;

  width: 30%;

  background-color: #49A0FF;

  color: white;

  align-self: center;

  font-size: 13px;

  border-radius: 60rpx;

  margin-top: 500rpx;

}
.imgs:nth-of-type(3){
  margin-left: 131rpx;
}