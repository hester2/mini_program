<view class="gn-page">
  <view class="gn-user-info-detail-container gn-app-fs28">
    <view class="gn-info-detail-wrap">
      <text class="gn-info-detail-title">基本信息</text>
      <view class="gn-info-detail-items">
        <view class="gn-info-detail-item">
          <view class="gn-user-detail-center">真实姓名</view>
          <view class="gn-app-fs24 gn-info-detail-item-content">{{baseInfo.name}}</view>
        </view>
        <view class="gn-info-detail-item" bindtap="onClickGetId">
          <view>Email</view>
          <view class="gn-app-fs24 gn-info-detail-item-content">{{baseInfo.email}}</view>
        </view>
      </view>
      <view class="gn-info-detail-items">
        <view class="gn-info-detail-item">
          <view>证件类型</view>
          <view class="gn-app-fs24 gn-info-detail-item-content">{{baseInfo.licenseType}}</view>
        </view>
        <view class="gn-info-detail-item">
          <view>证件号码</view>
          <view class="gn-app-fs24 gn-info-detail-item-content">{{baseInfo.licenseIdNum}}</view>
        </view>
      </view>
      <view class="gn-info-detail-items">
        <view class="gn-info-detail-item">
          <view>文化水平</view>
          <view class="gn-app-fs24 gn-info-detail-item-content">{{baseInfo.eduLevel}}</view>
        </view>
        <view class="gn-info-detail-item">
          <view>单位</view>
          <view class="gn-app-fs24 gn-info-detail-item-content">{{baseInfo.company}}</view>
        </view>
      </view>
      <view class="gn-info-detail-items" style="border:none;">
        <view class="gn-info-detail-item">
          <view>职业</view>
          <view class="gn-app-fs24 gn-info-detail-item-content">{{baseInfo.occupation}}</view>
        </view>
        <view class="gn-info-detail-item">
          <view>籍贯</view>
          <view class="gn-app-fs24 gn-info-detail-item-content">{{baseInfo.nativePlace}}</view>
        </view>
      </view>
    </view>


    <view class="gn-info-detail-wrap">
      <text class="gn-info-detail-title">健康信息</text>
      <view class="gn-info-detail-items">
        <view class="gn-info-detail-item">
          <view>年龄</view>
          <view class="gn-app-fs24 gn-info-detail-item-content">{{healthInfo.age}}</view>
        </view>
        <view class="gn-info-detail-item">
          <view>性别</view>
          <view class="gn-app-fs24 gn-info-detail-item-content">{{healthInfo.sex}}</view>
        </view>
      </view>
      <view class="gn-info-detail-items">
        <view class="gn-info-detail-item">
          <view>身高</view>
          <view class="gn-app-fs24 gn-info-detail-item-content">{{healthInfo.stature}}</view>
        </view>
        <view class="gn-info-detail-item">
          <view>体重</view>
          <view class="gn-app-fs24 gn-info-detail-item-content">{{healthInfo.weight}}</view>
        </view>
      </view>
      <view class="gn-info-detail-items">
        <view class="gn-info-detail-item">
          <view>血型</view>
          <view class="gn-app-fs24 gn-info-detail-item-content">{{healthInfo.bloodType}}</view>
        </view>
        <view class="gn-info-detail-item">
          <view>SOS联系电话</view>
          <view class="gn-app-fs24 gn-info-detail-item-content">{{healthInfo.sosPhone}}</view>
        </view>
      </view>
      <view class="gn-info-detail-items" style="border:none;">
        <view class="gn-info-detail-item">
          <view>备注信息</view>
          <view class="gn-app-fs24 gn-info-detail-item-content">{{healthInfo.remark}}</view>
        </view>
        <view class="gn-info-detail-item">
          <view></view>
          <view class="gn-app-fs24 gn-info-detail-item-content"></view>
        </view>
      </view>
    </view>
  </view>
</view>