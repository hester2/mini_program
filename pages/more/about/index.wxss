.gn-about-logo {
  width: 442rpx;
  height: 145rpx;
  margin: 72rpx 154rpx 54rpx 154rpx;
}

.gn-about-main {
  display: flex;
  flex-direction: column;
  padding: 39rpx 36rpx; 
  box-shadow: 0px 0px 4px 4px rgba(137, 137, 137, 0.05);
  border-radius: 10rpx;
  margin: 0 28rpx 24rpx 28rpx;
}

.gn-main-title {
  color: #46a0ff;
  margin-bottom: 23rpx;
}
.gn-main-content{
   color: #4d4d4d;
}

.gn-about-footer {
  display: flex;
  flex-direction: column;
  box-shadow: 0px 0px 4px 4px rgba(137, 137, 137, 0.05);
  border-radius: 10rpx;
  margin: 0 28rpx 128rpx 28rpx;
}
.gn-about-footer-item {
  display: flex;
  flex-direction: column;
  border-bottom: 3rpx solid #f2f2f2;
  margin-left: 36rpx;
  margin-right: 36rpx;
}
.gn-about-footer-name{
  border:none;
}
.gn-footer-item-name {
  color: #49a0ff;
  margin-bottom: 24rpx;
  margin-top: 40rpx;
}
.gn-footer-item-content {
  margin-bottom: 32rpx;
}