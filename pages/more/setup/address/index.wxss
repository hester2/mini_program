/* pages/more/setup/address/index.wxss */
.container{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  padding: 28rpx 28rpx;
}
.container image{
  width: 270rpx;
  height: 239rpx;
  margin-top: 300rpx;
}
.yh-address-fo{
  color: #4D4D4D;
  
  margin: 25rpx 0px;
}
.grace-null{
  position: fixed;
  right: 63rpx;
  bottom: 242rpx;
}
.grace-null image{
  width: 73rpx;
  height: 73rpx;
}
.yh-chives-main{
  box-sizing: border-box;
  width: 100%;
  box-shadow: 0px 0px 4px 4px rgba(137, 137, 137, 0.05);
  padding: 30rpx 28rpx;
  border-radius: 10rpx;
}
.grace-border-b{
  border-bottom: 1px solid #F8F8F8;
  color: #808080;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  padding-bottom: 20rpx;
}
.yh-address-desc{
  height: 50rpx;
  line-height: 50rpx;
  color: #4D4D4D
}
.yh-address-desc-size{
  font-size: 22rpx;
  color: #B3B3B3
}
.grace-form-items{
  display: flex;
  justify-content: space-between;
  margin-top: 25rpx;
}
.grace-form-col{
  display: flex;
  color: #4D4D4D !important;
}
.yh-form-icon{
  color: #4D4D4D;
  margin-left: 25rpx;
}
