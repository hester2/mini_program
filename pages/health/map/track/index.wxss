page {
  height: 100%;
}

.gn-track-wrap {
  width: 100%;
  height: 100%;
}

map {
  width: 100%;
  height: 100%;
}

.gn-track-time-wrap {
  width: 74rpx;
  height: 74rpx;
  border: 1rpx solid #fff;
  background: #fff;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  top: 28rpx;
  right: 28rpx;
}

.gn-track-time {
  width: 37rpx;
  height: 37rpx;
}

.gn-track-paly-item-wrap {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  position: fixed;
  bottom: 94rpx;
  left: 28rpx;
}

.gn-track-play-wrap {
  width: 694rpx;
  height: 75rpx;
  border: 1rpx solid #fff;
  background: #fff;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.gn-track-paly-item-wrap.gn-track-paly-active {
  bottom: 400rpx;
}

.gn-track-play {
  width: 31rpx;
  height: 36rpx;
  margin-left: 26rpx;
}

.gn-track-paly-line {
  width: 380rpx;
  height: 4rpx;
  border-radius: 3rpx;
  background: #49a0ff;
}

.gn-track-paly-speed-buttons {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.gn-track-paly-speed {
  color: #fff;
  width: 88rpx;
  height: 42rpx;
  line-height: 42rpx;
  text-align: center;
  border-radius: 28rpx;
  background: #49a0ff;
  margin-bottom: 26rpx;
  margin-right: 20rpx;
}

.gn-track-paly-speed.current-speed {
  margin-bottom: 0;
}

.gn-mask-detail-wrap {
  position: absolute;
  bottom: 28rpx;
  left: 28rpx;
  z-index: 9999;
  width: 750rpx;
  height: 360rpx;
}

.gn-detial-wrap {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  width: 694rpx;
  background: #fff;
  box-shadow: 0px 0px 4px 4px rgba(137, 137, 137, 0.05);
  border-radius: 10rpx;
  z-index: 9999;
  position: fixed;
  bottom: 94rpx;
  left: 28rpx;
}

.gn-detial-items-wrap {
  display: flex;
  align-items: center;
  width: 100%;
  padding-left: 45rpx;
  padding-right: 45rpx;
  box-sizing: border-box;
  justify-content: space-between;
}

.gn-detial-items-wrap.gn-detial-items-wrap-bg {
  background: #f1f4fa;
}

.gn-detial-item {
  display: flex;
  align-items: center;
  width: 50%;
}

.gn-detial-content {
  margin-top: 14rpx;
  margin-bottom: 14rpx;
  margin-left: 17rpx;
  text-align: left;
  white-space: normal;
  width: 280rpx;
}

.gn-detial-content-address.gn-detial-content {
  width: 588rpx;
}

.gn-map-high-icon {
  width: 13rpx;
  height: 24rpx;
  margin-right: 7rpx;
}

.gn-map-speed-icon {
  width: 17rpx;
  height: 27rpx;
}

.gn-map-direction-icon, .gn-map-clock-icon {
  width: 23rpx;
  height: 23rpx;
}

.gn-map-status-icon {
  width: 24rpx;
  height: 23rpx;
}

.gn-map-address-icon {
  width: 20rpx;
  height: 26rpx;
}
